@page
@model deneme_mvccore.Pages.Studentp.IndexModel
@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers
@{
}

<h1>Students</h1>
<table>
    <tr>
        <td>Id</td>
        <td>FirstName</td>
        <td>LastName</td>
        <td>Email</td>
    </tr>
    @foreach (var item in Model.Students)
    {
<tr>
    <td>@item.Id</td>
    <td>@item.FirstName</td>
    <td> @item.LastName</td>
    <td>@item.Email</td>

</tr>
    }

</table>
<form method="post">
    First Name: <input asp-for="Student.FirstName" /><br>
    Last Name: <input asp-for="Student.LastName" /><br>
    Email: <input asp-for="Student.Email" /><br>
    <input type="submit" />
</form>
